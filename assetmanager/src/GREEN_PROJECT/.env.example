# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_LOCATION=us-central1
GCP_BUCKET_NAME=${GCP_PROJECT_ID}-data

# Vertex AI Configuration
VERTEX_PROJECT_ID=${GCP_PROJECT_ID}
VERTEX_LOCATION=${GCP_LOCATION}
VERTEX_MODEL_REGION=us-central1
LLM_MODEL=gemini-2.5-pro-preview-05-06
LLM_THINKING_BUDGET=10000

# Optional API key authentication (alternative to service account)
# GOOGLE_API_KEY=your-api-key

# Neo4j Configuration
NEO4J_URI=neo4j+s://your-instance-id.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-password
NEO4J_DATABASE=neo4j

# Document AI Configuration
DOCAI_FORM_PROCESSOR_ID=your-form-processor-id
DOCAI_OCR_PROCESSOR_ID=your-ocr-processor-id
DOCAI_SPLITTER_PROCESSOR_ID=your-splitter-processor-id
DOCAI_QUALITY_PROCESSOR_ID=your-quality-processor-id

# Logging Configuration
LOG_LEVEL=INFO
